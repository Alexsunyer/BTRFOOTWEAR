  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "start": "node lib/app.js",
    "dev": "ts-node-dev --poll --respawn --ignore-watch node_modules ./src/app.ts",
    "build": "babel src --out-dir lib --extensions .ts,.tsx && touch .env && cp .env lib/.env",
    "test": "jest --forceExit --runInBand",
    "createDatabase": "jest  1database --forceExit",
    "runDevTest": "jest 1database --forceExit && npm run dev",
    "docker_dev": "npm i && (docker container stop ecommerce-back-back-1 || VER>NUL) && docker system prune --volumes -f && (docker network create ecommerce-network || VER>NUL) && docker-compose -f docker-compose.dev.yml up --build"
  },
  "standardx": {
    "ignore": [
      "test"
    ]
  },
